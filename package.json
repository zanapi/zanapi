{
  "name": "zanapi",
  "version": "0.0.3",
  "description": "RESTful web API Documentation Generator",
  "author": "Anla Sheng <anlasheng@gmail.com>",
  "license": "MIT",
  "preferGlobal": true,
  "bin": "bin/zanapi",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zanapi/zanapi.git"
  },
  "bugs": {
    "url": "https://github.com/zanapi/zanapi/issues"
  },
  "scripts": {
    "test": "npm run jshint && mocha test/",
    "jshint": "jshint lib/ test/",
    "build": "bin/zanapi -i example/ -o tmp/",
    "check-updates": "npm-check-updates"
  },
  "keywords": [
    "api",
    "zanapi",
    "doc",
    "documentation",
    "rest",
    "restful"
  ],
  "engines": {
    "node": ">= 0.10.0"
  },
  "dependencies": {
    "apidoc-core": "~0.7.1",
    "fs-extra": "~0.28.0",
    "lodash": "~4.11.1",
    "markdown-it": "^6.0.1",
    "nomnom": "~1.8.1",
    "winston": "~2.2.0"
  },
  "devDependencies": {
    "jshint": "^2.9.2",
    "mocha": "~2.4.5",
    "npm-check-updates": "^2.6.3",
    "path-to-regexp": "^1.2.1",
    "semver": "^5.1.0",
    "should": "~8.3.1",
    "webfontloader": "^1.6.24"
  },
  "jshintConfig": {
    "camelcase": true,
    "curly": false,
    "eqeqeq": true,
    "forin": true,
    "latedef": false,
    "newcap": true,
    "undef": true,
    "unused": true,
    "trailing": true,
    "node": true,
    "browser": true,
    "predef": [
      "it",
      "describe",
      "before",
      "after"
    ]
  }
}
